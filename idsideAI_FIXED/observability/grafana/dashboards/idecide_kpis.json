{
  "title": "IDECIDE \u2014 Product KPIs",
  "panels": [
    {
      "type": "graph",
      "title": "API Request Latency (P95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "stat",
      "title": "Snapshots Created (5m)",
      "targets": [
        {
          "expr": "sum(increase(idecide_snapshots_total[5m]))"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "stat",
      "title": "Why Exports (5m)",
      "targets": [
        {
          "expr": "sum(increase(idecide_exports_why_total[5m]))"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "graph",
      "title": "Errors (rate)",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{code=~\"5..\"}[5m]))"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      }
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}